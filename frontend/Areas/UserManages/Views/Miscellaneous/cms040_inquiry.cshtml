<script type="text/javascript">
    const permissions = {
        AllowView: common.Boolean("@permissions.AllowView"), AllowNew: common.Boolean("@permissions.AllowNew"), AllowEdit: common.Boolean("@permissions.AllowEdit"), AllowDelete: common.Boolean("@permissions.AllowDelete"), AllowExport: common.Boolean("@permissions.AllowNew"), AllowPrint: common.Boolean("@permissions.AllowPrint"),
    }

    var messageDialog = new MessageDialog();
</script>
<div id="search-container" class="card bg-light">
    <div class="card-body m-2">
        <div class="row g-3 d-flex justify-content-lg-between">
            <div class="col-9">
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-12">
                        <label for="sc-module" class="form-label">
                            @Resources["CMS040", "SC001"]
                        </label>
                        <input id="sc-module" name="sc-module" class="form-control" type="text" placeholder="@Resources["CMS040", "SC001"]" />
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-12">
                        <label for="sc-status" class="form-label">
                            @Resources["CMS040", "SC002"]
                        </label>
                        <input id="sc-status" name="sc-status" class="form-control" type="text" placeholder="@Resources["CMS040", "SC002"]" />
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-12">
                        <label for="sc-miscellaneoustype" class="form-label">
                            @Resources["CMS040", "SC003"]
                        </label>
                        <input id="sc-miscellaneoustype" name="sc-miscellaneoustype" class="form-control" type="text" placeholder="@Resources["CMS040", "SC003"]" />
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-12">
                        <label for="sc-miscellaneousvalue" class="form-label">
                            @Resources["CMS040", "SC004"]
                        </label>
                        <input id="sc-miscellaneousvalue" name="sc-miscellaneousvalue" class="form-control" type="text" placeholder="@Resources["CMS040", "SC004"]" />
                    </div>
                </div>
            </div>
            <div class="col-lg-1 col-md-12 col-sm-12 relative-container">
                <div class="position-bottom-right button-search">
                    @Html.SearchButton().Events(ev => ev.Click("searchButtonClicked"))
                    @Html.ClearButton().Events(ev => ev.Click("clearButtonClicked"))
                </div>
            </div>
        </div>
    </div>
</div>

<div id="message-container" class="alert alert-danger animated fadeIn mt-1" style="display:none"></div>

<div id="grid-inquiry" class="mt-3"></div>

